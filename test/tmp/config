WAV=$HOME/audio/Vyvn/Flock/1/$PROJ.wav
TEMPO=84
BEAT=$(bc <<< "scale=4; (60/$TEMPO)")
BAR=$(bc <<< "scale=4;$BEAT*4")
LENGTH=$(bc <<< "scale=4;$BAR*24")
DUR=$(bc <<< "scale=4;$BAR*4")
START_TIME=$(bc <<< "scale=2;$BAR*5")

