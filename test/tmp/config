PROJ=flock
PROJPATH=$HOME/mmw/test/$PROJ
LILY=$PROJPATH/$PROJ.ly
WAV=$PROJ.wav
TEMPO=84
BEAT=$(bc <<< "scale=4; (60/$TEMPO)")
BAR=$(bc <<< "scale=4;$BEAT*4")
DUR=$(bc <<< "scale=4;$BAR*16")
LENGTH=$(bc <<< "scale=2;$BAR*32+$BEAT")
START_TIME=$(bc <<< "scale=2;$BAR*0")

SYNTH=synth.ewf
BASS=bass.ewf
SNARE=snare.ewf
BRUSH=brush.ewf
HIHAT=hihat.ewf
KICK=kick.ewf

